                                        <TextBox x:Name="Mp_Copy5"
                                        materialDesign:HintAssist.Hint="版本详情" 
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="20,1" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Row="1" FontSize="20" Text="{Binding Version_Information, UpdateSourceTrigger=PropertyChanged}" Grid.Column="3"/>
                                    </Grid>
                                </Grid>
                            </TabItem>
                            <TabItem  Header="卡片指令">
