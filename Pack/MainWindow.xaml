            </TabItem>
            <TabItem x:Name="Menu_Skill_Cards" Header="技能设置">
                <Grid>
                    <Element:CardPanle x:Name="CardPanle"  Margin="0"/>
                </Grid>
            </TabItem>
            <TabItem x:Name="Menu_Adventure_Cards" Header="奇遇设置" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top" Width="57">
