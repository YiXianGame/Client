                                <Grid>
                                    <Element:Basic_CardPanle x:Name="BasicCardsPanle" Width="1312" Margin="0,-2,0,2" HorizontalAlignment="Left"/>
                                </Grid>
                            </TabItem>
                        </TabControl>
                    </Border>
