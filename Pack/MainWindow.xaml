                                        <TextBox x:Name="Mp_Copy9"
                                        materialDesign:HintAssist.Hint="出售" 
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="20,1" Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" Grid.Row="1" FontSize="20" Text="{Binding Sell_Card, UpdateSourceTrigger=PropertyChanged}" Grid.Column="1"/>
