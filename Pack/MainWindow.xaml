                                        <TextBox 
                                                materialDesign:HintAssist.Hint="预持上限" 
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="25" Text="{Binding Pre_Hold_Max, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="50,0,0,0"/>
                                    </WrapPanel>
                                </Grid>
