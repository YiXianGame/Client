                                        <TextBox x:Name="Probability2_Copy3"
                                        materialDesign:HintAssist.Hint="抽卡" 
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="20,1" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="20"  Text="{Binding Draw_Card, UpdateSourceTrigger=PropertyChanged}"/>
                                        <TextBox x:Name="Probability2_Copy4"
                                        materialDesign:HintAssist.Hint="查看库存" 
                                        Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Margin="20,1" Foreground="DarkTurquoise" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="20"  Text="{Binding View_Repository, UpdateSourceTrigger=PropertyChanged}" Grid.Column="2"/>
