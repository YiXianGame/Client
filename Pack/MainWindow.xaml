                                                materialDesign:HintAssist.Hint="购卡金额" 
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="25" Text="{Binding Buy_Card_Coast, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="80,0,50,0"/>
                                        <TextBox 
                                                materialDesign:HintAssist.Hint="抽卡金额" 
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="25" Text="{Binding Draw_Card_Coast, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="50,0"/>
                                        <TextBox 
                                                materialDesign:HintAssist.Hint="升级金额" 
                                                Style="{StaticResource MaterialDesignFloatingHintTextBox}"  Foreground="blue" FontFamily="楷体" HorizontalContentAlignment="Center" FontSize="25" Text="{Binding Upgrade_Card_Coast, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="50,0"/>
